{% extends "::base.html.twig"%}
{% block contenido%}
<body background="{{ asset('images/triangleDown.jpg') }}" class="mifondo">
  <form method="POST">
    <row class="col-xs-12">
      {% include 'FrontEndBundle:Widgets:WidgetTableLuz.html.twig' with {
        'cabeceras': datos.tablaLuz.cabeceras,
        'filas': datos.tablaLuz.filas} %}
    <div class="col-xs-6" style="margin-left:25%; margin-top:5%">
      {% include 'FrontEndBundle:Widgets:WidgetGastoEnCursoLuz.html.twig' with {
        'gastoEnCurso': datos.gastoLuz.valor} %}
    </div>
  </row>
</form>
<script type="text/javascript">
  $(document).ready(function(){
    $('#tablaLuz').DataTable({
      "language": {
        "url" : "{{asset('js/DataTableSpanish.json')}}"
      }
    });
  });
</script>
</body>
{% endblock %}  

