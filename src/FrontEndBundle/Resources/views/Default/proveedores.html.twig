{% extends "::base.html.twig"%}
{% block contenido%}
<body background="{{ asset('images/triangleDown.jpg') }}" class="mifondo">
  <form method="POST">
    <row class="col-xs-12">
      {% include 'FrontEndBundle:Widgets:WidgetTableProveedores.html.twig' with {
        'cabeceras': datos.tablaProveedores.cabeceras,
        'filas': datos.tablaProveedores.filas} %}
    <div class="col-xs-6" style="margin-left:25%; margin-top:5%">
      {% include 'FrontEndBundle:Widgets:WidgetGastoEnCursoProveedores.html.twig' with {
        'gastoEnCurso': datos.gastoProveedores.valor} %}
    </div>
  </row>
</form>
<script type="text/javascript">
  $(document).ready(function(){
    $('#tablaProveedores').DataTable({
      "language": {
        "url" : "{{asset('js/DataTableSpanish.json')}}"
      }
    });
  });
</script>
</body>
{% endblock %}  

